<main class="page manager-page">
    <section class="clean-block about-us">
        <div class="container-fluid">
            <div class="block-heading">
                <h2 class="text-info">Phản hồi từ thành viên</h2>
            </div>


            <div class="container-center ">



                <div class="tm-product-table-container ">
                    <table class="table table-hover tm-table-small tm-product-table ">
                        <thead>
                            <tr>

                                <th scope="col" class="tdtentv">TÊN THÀNH VIÊN</th>
                                <th scope="col" class="tdtentv">TIÊU ĐỀ</th>
                                <th scope="col" class="tdphanhoi">PHẢN HỒI</th>
                                <th scope="col">THỜI GIAN</th>
                            </tr>
                        </thead >
                        <tbody>
                            <% listContact.forEach(function(phanhoi) { %>   
                                <tr>         
                                        <td class="tdtentv"> <%= phanhoi.name %>  </td>
                                        <td class="tdtentv"> <%= phanhoi.title %> </td>
                                        <td class="tdphanhoi"> <%= phanhoi.content %></td>
                                        <td> <%= phanhoi.time %></td>
                                </tr>
                            <% }); %>

                        </tbody>
                    </table>


                </div>
            </div>
        </div>
    </section>

</main>